# Maintainer: ArseLinuxOS-Team

pkgname=arse-installer
pkgver=1.0
pkgrel=10
pkgdesc='ArseLinuxOS installer'
arch=('any')
license=('GPL3')
depends=(libnewt)

source=('installer' )

sha512sums=('99317d230d89aa6bf7880f89d6700926767c6dacdc744ee19a34f517c3518a167275dcce21a946958755ab8f68bec389b4718e56a91a2d9abb358305ec492c7e')

package() {
  local installer=$pkgdir/usr/bin/

  install -Dm755 installer      $installer/installer
}
