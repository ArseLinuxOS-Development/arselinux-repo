# Maintainer: ArseLinuxOS-Team

pkgname=arse-installer
pkgver=1.0
pkgrel=8
pkgdesc='ArseLinuxOS installer'
arch=('any')
license=('GPL3')
depends=(libnewt)

source=('installer' )

sha512sums=('f8724ee19d13cdf369792399afc7277bf372af0351489a23e4fb977868a5f13b88217578c4d048ecf439b94cc3876e20f720c72717629d37c50cdab87c4bb006')

package() {
  local installer=$pkgdir/usr/bin/

  install -Dm755 installer      $installer/installer
}
