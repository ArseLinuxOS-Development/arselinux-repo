# Maintainer: ArseLinuxOS-Team

pkgname=arse-installer
pkgver=1.0
pkgrel=7
pkgdesc='ArseLinuxOS installer'
arch=('any')
license=('GPL3')
depends=(libnewt)

source=('installer' )

sha512sums=('5b96a1dffaa91f70c191efe36b1436ea96924eb92387b44e91b6f1a232d931f6516512fc071d652b955b1293935073653f004ce74684ca83902621082598ee64')

package() {
  local installer=$pkgdir/usr/bin/

  install -Dm755 installer      $installer/installer
}
