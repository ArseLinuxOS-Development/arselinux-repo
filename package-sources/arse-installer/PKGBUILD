# Maintainer: ArseLinuxOS-Team

pkgname=arse-installer
pkgver=1.0
pkgrel=9
pkgdesc='ArseLinuxOS installer'
arch=('any')
license=('GPL3')
depends=(libnewt)

source=('installer' )

sha512sums=('f7584456b4b72510e6b1fbee45c19ab5af35f9085489f9de39d21fe23f12ad34d10994b3cf875ac7d7b7833116f23d0ac7aac2d57170c29d736ead5dffdd4465')

package() {
  local installer=$pkgdir/usr/bin/

  install -Dm755 installer      $installer/installer
}
